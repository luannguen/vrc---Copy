{
  "name": "vrc-backend",
  "version": "1.0.0",
  "private": true,
  "description": "VRC Backend",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "build": "cross-env NODE_OPTIONS=--no-deprecation next build",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs",
    "dev": "cross-env NODE_OPTIONS=--no-deprecation next dev",
    "dev:prod": "cross-env NODE_OPTIONS=--no-deprecation rm -rf .next && pnpm build && pnpm start",
    "generate:importmap": "cross-env NODE_OPTIONS=--no-deprecation payload generate:importmap",
    "generate:types": "cross-env NODE_OPTIONS=--no-deprecation payload generate:types",
    "ii": "cross-env NODE_OPTIONS=--no-deprecation pnpm --ignore-workspace install",
    "lint": "cross-env NODE_OPTIONS=--no-deprecation next lint",
    "lint:fix": "cross-env NODE_OPTIONS=--no-deprecation next lint --fix",
    "payload": "cross-env NODE_OPTIONS=--no-deprecation payload",
    "reinstall": "cross-env NODE_OPTIONS=--no-deprecation rm -rf node_modules && rm pnpm-lock.yaml && pnpm --ignore-workspace install",
    "seed": "cross-env NODE_OPTIONS=--no-deprecation ts-node src/scripts/seed.ts",
    "seed:js": "cross-env NODE_OPTIONS=--no-deprecation node scripts/run-seed.js",
    "start": "cross-env NODE_OPTIONS=--no-deprecation next start",
    "test:health": "node src/endpoints/test-health.js",
    "fix:products": "node src/app/api/clean-products.js",
    "delete:product": "node src/app/api/delete-product.js"
  },
  "dependencies": {
    "@payloadcms/admin-bar": "3.33.0",
    "@payloadcms/db-mongodb": "3.33.0",
    "@payloadcms/graphql": "^3.33.0",
    "@payloadcms/live-preview-react": "3.33.0",
    "@payloadcms/next": "^3.33.0",
    "@payloadcms/payload-cloud": "3.33.0",
    "@payloadcms/plugin-form-builder": "3.33.0",
    "@payloadcms/plugin-nested-docs": "3.33.0",
    "@payloadcms/plugin-redirects": "3.33.0",
    "@payloadcms/plugin-search": "3.33.0",
    "@payloadcms/plugin-seo": "3.33.0",
    "@payloadcms/richtext-lexical": "^3.33.0",
    "@payloadcms/ui": "^3.33.0",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "axios": "^1.9.0",
    "class-variance-authority": "^0.7.0",
    "cli-progress": "^3.12.0",
    "clsx": "^2.1.1",
    "colors": "^1.4.0",
    "cross-env": "^7.0.3",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.12.1",
    "geist": "^1.3.0",
    "graphql": "^16.8.2",
    "image-size": "^2.0.2",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.378.0",
    "next": "^15.3.2",
    "next-sitemap": "^4.2.3",
    "payload": "^3.33.0",
    "prism-react-renderer": "^2.3.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "7.45.4",
    "react-intersection-observer": "^9.16.0",
    "sharp": "0.32.6",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@tailwindcss/typography": "^0.5.13",
    "@types/cli-progress": "^3.11.6",
    "@types/escape-html": "^1.0.2",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/node": "22.5.4",
    "@types/react": "19.1.0",
    "@types/react-dom": "19.1.2",
    "autoprefixer": "^10.4.19",
    "copyfiles": "^2.4.1",
    "dotenv": "^16.5.0",
    "eslint": "^9.16.0",
    "eslint-config-next": "15.3.0",
    "node-fetch": "^3.3.2",
    "postcss": "^8.4.38",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.3",
    "typescript": "5.7.3"
  },
  "engines": {
    "node": "^18.20.2 || >=20.9.0",
    "pnpm": "^9 || ^10"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp"
    ]
  }
}
